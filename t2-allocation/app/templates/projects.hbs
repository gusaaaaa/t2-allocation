<section id="projectList">
  {{#each project in model}}
    {{#if project.availableProject}}
      {{ render 'available-project' project }}
    {{else}}
      {{#if project.dummyProject}}
        {{ render 'dummy-project' project }}
      {{else}}
        {{ render 'project' project }}
      {{/if}}
    {{/if}}
  {{/each}}
</section>
